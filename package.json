{
  "name": "base-project",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index",
  "scripts": {
    "start": "node ./dist/src/index.js",
    "start-dev": "ts-node ./src/index.ts",
    "build": "npm run build:clean && npm run build:lib && npm run lint",
    "build:ci": "npm run lint && npm run build && npm run test && npm run test:e2e",
    "build:clean": "rimraf dist",
    "build:lib": "tsc --declaration",
    "build:exposed": "tsc --skipLibCheck true -d src/exposed_interfaces.ts",
    "test": "nyc --nycrc-path .nyrc_unit npm run test:unit",
    "test:full": "npm run lint && npm run build && npm run test && npm run test:integration",
    "test:unit": "mocha --compilers ts-node/register --require source-map-support/register --full-trace \"tests/unit/**/*_test.ts\"",
    "test:integration": "nyc --nycrc-path .nyrc_e2e mocha --compilers ts-node/register --require source-map-support/register --full-trace \"tests/integration/**/*_test.ts\" --exit --timeout 50000",
    "lint": "tslint -p tsconfig-lint.json \"src/**/*.ts\"  \"tests/**/*.ts\"",
    "lint-fix": "tslint -p tsconfig-lint.json --fix \"src/**/*.ts\" \"tests/**/*.ts\"",
    "migrate": "sequelize db:migrate",
    "newmigration": "sequelize migration:create --name $1"
  },
  "repository": {
    "type": "git",
    "url": "git clone git@bitbucket.org:onebytetech/base-project.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/chai-as-promised": "^7.1.0",
    "@types/dotenv": "^4.0.3",
    "@types/elasticsearch": "^5.0.19",
    "@types/express": "^4.0.36",
    "@types/ioredis": "0.0.24",
    "@types/lodash": "^4.14.91",
    "@types/mocha": "^2.2.45",
    "@types/node": "^8.0.17",
    "@types/proxyquire": "^1.3.28",
    "@types/request-promise-native": "^1.0.14",
    "@types/sequelize": "^4.27.1",
    "@types/sinon": "^4.3.1",
    "@types/sqlite3": "^3.1.3",
    "@types/string": "^0.0.29",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-shallow-deep-equal": "^1.4.6",
    "chai-subset": "^1.6.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "proxyquire": "^2.1.0",
    "request": "^2.85.0",
    "rimraf": "*",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.2.0",
    "sinon-chai-in-order": "^0.1.0",
    "sqlite3": "^4.0.2",
    "supertest": "^3.0.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.11.0",
    "tslint-config-globalid": "^1.0.6",
    "typescript": "^2.9.2",
    "ts-transformer-keys": "^0.3.3",
    "typescript-eslint-parser": "^11.0.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ],
    "globals": [
      "sandbox",
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  },
  "dependencies": {
    "@awspilot/dynamodb": "^1.0.5",
    "dotenv": "^4.0.0",
    "lodash": "^4.17.5",
    "micro-kit-atlas": "^5.1.2",
    "moment": "^2.18.1",
    "mysql": "^2.13.0",
    "opentracing": "^0.14.3",
    "querystring": "^0.2.0",
    "request-promise-native": "^1.0.5",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "uuid": "~3.0.1"
  }
}
